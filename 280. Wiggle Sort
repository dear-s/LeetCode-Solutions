

class Solution:
    def wiggleSort(self, nums: List[int]) -> None:
        """
        Do not return anything, modify nums in-place instead.
        """
		
		# premium question
        
        # sort
        # swap with adjacent element
        
        nums.sort()
        
        for i in range(1, len(nums)-1, 2): # skipping one
            # swap
            temp = nums[i]
            nums[i] = nums[i+1]
            nums[i+1] = temp
            
        print("nums: ", nums)

	
