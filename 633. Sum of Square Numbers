import math

class Solution:
    def judgeSquareSum(self, c: int) -> bool:
        
        n = 0
        dict_ = {}
        while n*n <= c:
            a2 = n*n
            dict_[a2] = n
            
            if c-a2 in dict_:
                return True
            
            n+=1
        
        
        
