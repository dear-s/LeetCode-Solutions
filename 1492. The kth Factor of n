
class Solution:
    def kthFactor(self, n: int, k: int) -> int:
        
        
        # 1 to n
        arr = []
        count = 0
        
        for i in range(1, n+1):
            if n%i == 0:
                arr.append(i)
            
#                 count += 1
            
#             if count == k:
#                 return arr[-1]
             
        
        if k <= len(arr):
            return arr[k-1]
        else:
            return -1
        
